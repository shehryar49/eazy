var lowerLimit 5.0
var upperLimit 10.0
var steps 100000.0

var stepSize upperLimit - lowerLimit
set stepSize stepSize / steps

var sum 0.0
var i lowerLimit
while( i < steps )
	var x lowerLimit + i * stepSize
	set sum sum + 1.0 / x
	set i i + 1.0
end

printstr "Integration of 1/x from 5 to 10 is:"
print sum * stepSize